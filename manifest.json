{
	"name": "TimeMaster",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Timer for the lazy people",
    "background": {
        "service_worker": "background.js"   
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "alarms", "notifications"]
}